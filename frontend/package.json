{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://guswls6212.github.io/class_planner",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "clean": "eslint . --fix && prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "auto-cleanup": "./scripts/auto-cleanup.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:html": "vitest --coverage --reporter=html",
    "test:coverage:lcov": "vitest --coverage --reporter=lcov",
    "test:coverage:ci": "vitest run --coverage --reporter=text",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:protection": "npm run test:run -- project-protection",
    "protection-check": "chmod +x scripts/protection-check.sh && ./scripts/protection-check.sh",
    "prepare-commit": "npm run protection-check && npm run format",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "setup-env": "./setup-env.sh",
    "dev:setup": "npm run setup-env && npm run dev"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.57.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "uid": "^2.0.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/html2canvas": "^0.5.35",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
