{
  "name": "class-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:real-scenarios": "playwright test tests/e2e/real-user-scenarios.spec.ts",
    "test:e2e:browser-compatibility": "playwright test tests/e2e/browser-compatibility.spec.ts",
    "test:integration:real-supabase": "vitest run tests/integration/real-supabase.test.ts",
    "test:integration:real-data": "vitest run tests/integration/real-data-validation.test.tsx",
    "test:real-client": "npm run test:integration:real-supabase && npm run test:integration:real-data && npm run test:e2e:real-scenarios",
    "test:system": "node scripts/system-test.js",
    "test:system:headless": "headless=true node scripts/system-test.js",
    "type-check": "tsc --noEmit",
    "prepare-commit": "chmod +x scripts/protection-check.sh && ./scripts/protection-check.sh",
    "migration:run": "./migration/run-migration.sh",
    "migration:status": "echo 'Migration 상태를 확인하려면 Supabase SQL Editor에서 다음 쿼리를 실행하세요: SELECT * FROM public.get_migration_status();'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "html2canvas": "^1.4.1",
    "jsdom": "^26.1.0",
    "jspdf": "^3.0.2",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.8.2",
    "uid": "^2.0.2",
    "uuid": "^13.0.0",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/html2canvas": "^0.5.35",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^6.0.1"
  }
}
